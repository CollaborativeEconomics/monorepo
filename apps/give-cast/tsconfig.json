{
  "extends": "@cfce/typescript-config/nextjs.json",
  "strictNullChecks": true,
  "declaration": false,
  "compilerOptions": {
    "baseUrl": ".",
    "rootDirs": ["src", "."],
    "outDir": "dist",
    "paths": {
      "~/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
